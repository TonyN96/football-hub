{{> menu }}

<div class="container">
    <table class="table table-dark" id="fixture-list">
        <thead>
            <tr>
                <th>Date</th>
                <th>Home</th>
                <th>Away</th>
            </tr>
            <tbody>
            {{#each fixtureList}}
            <tr>
                <td>{{date}}</td>
                <td>{{homeTeam}}</td>
                <td>{{awayTeam}}</td>
            </tr>
            {{/each}}
            </tbody>
        </thead>
    </table>
</div>