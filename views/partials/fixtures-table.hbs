<div class="row text-center p-4 text-light"><h2>Gameweek {{gameweek}}</h2></div>
<table class="table table-striped table-dark" id="fixture-list">
        <thead>
            <th style="text-align: right;">Home</th>
            <th></th>
            <th>Away</th>
        </thead>
        <tbody>
        {{#each fixtureList}}
        <tr>
            <td style="text-align: right;">{{homeTeam}}<img src="img/crests/{{homeTeam}}.png" height="25" width="25"  style="margin-left: 20px;"></td>
            <td style="text-align: center;">{{result}}</td>
            <td><img src="img/crests/{{awayTeam}}.png" height="25" width="25" style="margin-right: 20px;">{{awayTeam}}</td>
        </tr>
        {{/each}}
        </tbody>
</table>