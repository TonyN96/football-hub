<div class="row text-center text-light"><h2>Gameweek {{gameweek}}</h2></div>
<table class="table table-striped table-dark table-hover table-responsive" id="fixture-list">
        <thead>
            <tr class="row">
                <th class="col-sm-5" style="text-align: right;">Home</th>
                <th class="col-sm-2" ></th>
                <th class="col-sm-5">Away</th>
            </tr>
        </thead>
        <tbody>
        {{#each fixtureList}}
            <tr class="row">
                <td class="col-sm-5" style="text-align: right;">{{homeTeam}}<img src="img/crests/{{homeTeam}}.png" height="25" width="25"  style="margin-left: 20px;"></td>
                <td class="col-sm-2" style="text-align: center;">{{result}}</td>
                <td class="col-sm-5"><img src="img/crests/{{awayTeam}}.png" height="25" width="25" style="margin-right: 20px;">{{awayTeam}}</td>
            </tr>
        {{/each}}
        </tbody>
</table>